app_identifier(ENV["DEVELOPER_APP_IDENTIFIER"]) # The bundle identifier of your app (used in pilot, match)
apple_id(ENV["FASTLANE_APPLE_ID"]) # Your Apple Developer Portal username (used in pilot)

itc_team_id(ENV["FASTLANE_ITC_TEAM_ID"]) # App Store Connect Team ID
team_id(ENV["DEVELOPER_PORTAL_TEAM_ID"]) # Developer Portal Team ID

